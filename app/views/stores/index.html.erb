<p style="color: green"><%= notice %></p>

<h1>Stores</h1>

<div id="stores">
  <table border="1">
    <thead>
      <tr>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
      <tbody>
        <% @stores.each do |store| %>
          <%= render store %>
        <% end %>
      </tbody>
    </thead>
  </table>
</div>

<%= link_to "New store", new_store_path %>
